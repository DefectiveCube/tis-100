# 163 CYCLES
# READ VALUES FROM IN.A AND IN.B
# WRITE IN.A - IN.B TO OUT.P
# WRITE IN.B - IN.A TO OUT.N

#A2
MOV UP ACC
SUB RIGHT
MOV ACC DOWN
MOV UP RIGHT

#A3
MOV UP LEFT
MOV UP ACC
SUB LEFT
MOV ACC DOWN

#B2
MOV UP ACC
MOV ACC RIGHT
MOV ACC DOWN
MOV RIGHT DOWN

#B3
MOV LEFT DOWN
MOV UP ACC
MOV ACC LEFT
MOV ACC DOWN

#C2
MOV UP DOWN
MOV UP ACC
NEG
MOV ACC DOWN

#C3
MOV UP ACC
NEG
MOV ACC DOWN
MOV UP DOWN
